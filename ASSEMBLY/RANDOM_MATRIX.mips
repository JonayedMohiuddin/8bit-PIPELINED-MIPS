
addi $t2, $zero 32
addi $t0, $zero, 0 # initial x at 0
draw:

repeat:
lw $t1, 15($io) # load random input for y value
sw $t0, 0($io) # store x value
sw $t1, 1($io) # store y value
sw $zero, 2($io) # update flag
addi $t0, $t0, 1 # increment x by 1
beq $t0, $t2, draw # if x = 32, go to draw
j repeat # repeat
draw:
sw $zero, 3($io) # flush flag
addi $t0, $zero, 0 # reset x to 0
j repeat 