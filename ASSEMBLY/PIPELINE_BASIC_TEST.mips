addi $sp $zero -2
sw $t0 $sp 0
lw $t1 $sp 0

addi $t0 $zero 1 # $t0 = 1
addi $t1 $t0 1   # $t1 = $t0 + 1 = 2
addi $t2 $t1 1   # $t2 = $t1 + 1 = 3
addi $t3 $t2 1   # $t3 = $t2 + 1 = 4
addi $t4 $t3 1   # $t4 = $t3 + 1 = 5
addi $sp $t4 1   # $sp = $t4 + 1 = 6

add $t0 $t4 $sp # $t0 = $t4 + $sp = 5 + 6 = 11
add $t1 $t2 $t3 # $t1 = $t2 + $t3 = 3 + 4 = 7

sll $t0 $t1 2 # $t0 = $t1 << 2 = 7 << 2 = 28
srl $t1 $t4 1 # $t1 = $t2 >> 1 = 3 >> 1 = 1

